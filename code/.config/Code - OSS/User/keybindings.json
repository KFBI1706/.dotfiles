// Place your key bindings in this file to override the defaultsauto[]
[
    {
        "key": "ctrl+shift+k",
        "command": "workbench.action.toggleActivityBarVisibility"
    },
    {
        "key": "ctrl+enter",
        "command": "workbench.action.terminal.focus"
    },
    {
        "key": "ctrl+enter",
        "command": "workbench.action.terminal.focus"
    },
    {
        "key": "ctrl+enter",
        "command": "workbench.action.focusActiveEditorGroup",
        "when": "terminalFocus"
    },
    {
        "key": "ctrl+h",
        "command": "workbench.action.navigateLeft"
    },
    {
        "key": "ctrl+l",
        "command": "workbench.action.navigateRight"
    },
    {
        "key": "ctrl+k",
        "command": "workbench.action.navigateUp"
    },
    {
        "key": "ctrl+j",
        "command": "workbench.action.navigateDown"
    },
    {
        "key": "ctrl+shift+b",
        "command": "cargo.build",
        "when": "editorLangId == rust"
    },
    {
        "key": "ctrl+shift+b",
        "command": "latex-workshop.recipes",
        "when": "editorLangId == latex"
    },
    {
        "key": "ctrl+shift+t",
        "command": "workbench.view.extension.test"
    },
    {
        "key": "ctrl+e",
        "command": "-workbench.action.quickOpen"
    },
    {
        "key": "ctrl+n",
        "command": "explorer.newFile"
    },
    {
        "key": "ctrl+shift+down",
        "command": "workbench.action.terminal.resizePaneDown",
        "when": "terminalFocus"
    },
    {
        "key": "ctrl+shift+up",
        "command": "workbench.action.terminal.resizePaneUp",
        "when": "terminalFocus"
    },
    {
        "key": "ctrl+'",
        "command": "settings.cycle.lineNumbers",
    },
    {
        "key": "ctrl+m",
        "command": "-extension.vim_ctrl+m",
        "when": "editorTextFocus && vim.active && vim.use<C-m> && !inDebugRepl || vim.active && vim.use<C-m> && !inDebugRepl && vim.mode == 'CommandlineInProgress' || vim.active && vim.use<C-m> && !inDebugRepl && vim.mode == 'SearchInProgressMode'"
    },
    {
        "key": "ctrl+shift+capslock ctrl+shift+m",
        "command": "extension.vim_ctrl+m",
        "when": "editorTextFocus && vim.active && vim.use<C-m> && !inDebugRepl || vim.active && vim.use<C-m> && !inDebugRepl && vim.mode == 'CommandlineInProgress' || vim.active && vim.use<C-m> && !inDebugRepl && vim.mode == 'SearchInProgressMode'"
    },
    {
        "key": "ctrl+shift+capslock ctrl+shift+q",
        "command": "workbench.action.duplicateWorkspaceInNewWindow"
    },
    {
        "key": "f9",
        "command": "-editor.debug.action.toggleBreakpoint",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+shift+q",
        "command": "workbench.action.duplicateWorkspaceInNewWindow"
    }
]