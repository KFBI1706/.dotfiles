#!/usr/bin/env python2
from pwn import *
from pwnlib.util.packing import *

local = True

elf = ELF("$1")

if local:
	r = elf.process()
	libc = ELF('${2:/usr/lib32/libc.so.6}')
else:
	r = remote("${4:remote}",${5:69})
	libc = ELF('${6:./libc}')

print(r.recv())

